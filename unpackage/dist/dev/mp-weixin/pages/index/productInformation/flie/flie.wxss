.shop_select_right_item {
  margin-bottom: 10px;
  margin-right: 10px;
  width: 125px;
  height: 125px;
  background: #F5F5F5;
  border-radius: 4px;
  font-size: 12px;
  font-weight: 400;
  color: #000000;
  line-height: 17px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.shop_select_right_item image {
  width: 50px;
  height: 40px;
  margin-bottom: 15px;
}
.shop_select_right {
  display: flex;
  flex-wrap: wrap;
}

